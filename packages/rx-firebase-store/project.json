{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "packages/rx-firebase-store/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/rx-firebase-store",
        "tsConfig": "packages/rx-firebase-store/tsconfig.lib.json",
        "project": "packages/rx-firebase-store/package.json",
        "entryFile": "packages/rx-firebase-store/src/index.ts",
        "external": [],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/rx-firebase-store/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/rx-firebase-store/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/rx-firebase-store"],
      "options": {
        "jestConfig": "packages/rx-firebase-store/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
